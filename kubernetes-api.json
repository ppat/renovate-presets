{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "addLabels": [
        "automerge:off",
        "dependency-type:kubernetes-api"
      ],
      "automerge": false,
      "commitBody": "BREAKING CHANGE",
      "commitMessagePrefix": "feat(kubernetes-api)!:",
      "description": "datasource=kubernetes-api | configure automerge, breaking change, labels, grouping and  min release age",
      "groupName": "kubernetes-api-{{packageName}}",
      "matchDatasources": [
        "kubernetes-api"
      ],
      "minimumReleaseAge": "60 days",
      "reviewers": [
        "ppat"
      ],
      "semanticCommits": "disabled"
    }
  ]
}
